{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true
  },
  "extends": [
    "plugin:sonarjs/recommended",
    "@frontenso/javascript",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/no-deprecated": "off",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  },
  "plugins": ["sonarjs"]
}
