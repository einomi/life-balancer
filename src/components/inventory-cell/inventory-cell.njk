{% from '../popup/popup.njk' import popup %}

{% macro inventoryCell(props) %}
  <div class="inventory__cell inventory-cell" data-inventory-cell>
    {% if props.id %}
      <img src="/shop/{{ props.id }}.png" alt="" />
    {% endif %}
    {{
      popup({
        description: '',
        price: ''
      })
    }}
  </div>
{% endmacro %}
